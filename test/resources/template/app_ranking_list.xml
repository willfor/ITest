			<column name="downloads_deviation" defaultValue="27" type="int(11)" />
			<column name="month_downloads" defaultValue="1256" type="int(11)" />
			<column name="month_deviation" defaultValue="22" type="int(11)" />
		</columns>
	</table>
</tables>
